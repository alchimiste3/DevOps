<?xml version="1.0" encoding="UTF-8"?>
<!-- liste des mutants à appliquer-->
<configuration>
  <mutants>
    <mutant>
      <processor>PublicToPrivate</processor>
    </mutant>
        <mutant>
      <processor>PlusMinus</processor>
    </mutant>
  </mutants>

  <processors>
    <processor>
      <nom>PlusMinus</nom><!-- s'applique sur la classe Classe1-->
          <package>
            <nom>package2</nom>
            <classe>
              <nom>Classe2</nom>
            </classe>
          </package>
          <applications>4</applications>
    </processor>
  </processors>
<!--GELE et DoubleMutator n'ont pas de configuration définie, donc s'applique tout le temps partout où ils peuvent -->

</configuration>